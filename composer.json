    "type": "library",
    "require": {
        "php": ">=7.2.0",
        "php-64bit": "*",
        "pocketmine/binaryutils": "dev-master"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.29",
        "irstea/phpunit-shim": "^7.5 || ^8.0"
    },
    "license": "LGPL-3.0",
    "autoload": {
        "psr-4": {
            "pocketmine\\nbt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "pocketmine\\nbt\\": "tests/phpunit/"
        }
    }
}
